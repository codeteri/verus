<div class=“bookmarked-heading”><h3>Search Results</h3></div>
<div class=“bookmarked”>
  <% @results.each do |article| %>
    <div class=“article-card”>
      <div class=“vote-overlay” style=“<%= article.leaning_style %>“> <%= article.leaning_string %> </div>
      <img src=“<%= article.photo_url %>” alt=“article pic”>
      <%#= image_tag(article.photo_url, if article.photo_url? %>
      <h5><%= link_to article.title, article_path(article) %></h5>
    </div>
  <% end %>
</div>